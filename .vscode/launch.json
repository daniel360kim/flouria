    "version": "0.2.0",
    "configurations": [

        {
            "name": "flourish_web",
            "request": "launch",
            "type": "dart",
            "args": [
                "--web-browser-flag=--disable-web-security",
                
                "--web-port",
                "8080",
       
            ]
        },
        {
            "name": "flourish_web (profile mode)",
            "request": "launch",
            "type": "dart",
            "flutterMode": "profile"
        },
        {
            "name": "flourish_web (release mode)",
            "request": "launch",
            "type": "dart",
            "flutterMode": "release"
        }
    ]
}